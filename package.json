{
  "name": "@wrtnlabs/eslint-config",
  "type": "module",
  "version": "0.3.1",
  "packageManager": "pnpm@10.6.4",
  "author": "Wrtn Technologies",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wrtnlabs/eslint-config.git"
  },
  "exports": {
    ".": "./src/index.ts",
    "./next": "./src/next.ts"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "./next": {
        "types": "./dist/next.d.ts",
        "default": "./dist/next.mjs"
      },
      "./package.json": "./package.json"
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint --cache .",
    "format": "nr lint --fix",
    "inspector": "nlx @eslint/config-inspector",
    "prepublishOnly": "nr lint && nr build && nlx publint",
    "release": "nr lint && nr build && nlx publint && nlx bumpp"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": "^15.2.4",
    "eslint": ">= 9.0.0",
    "eslint-plugin-tailwindcss": "^3.18.0"
  },
  "peerDependenciesMeta": {
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-config": "^4.10.2",
    "defu": "^6.1.4",
    "pkg-types": "^2.1.0"
  },
  "devDependencies": {
    "@antfu/ni": "^24.3.0",
    "@next/eslint-plugin-next": "^15.2.4",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "bumpp": "^10.1.0",
    "eslint": "^9.22.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "importx": "^0.5.2",
    "mkdist": "^2.2.0",
    "publint": "^0.3.9",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0"
  }
}
